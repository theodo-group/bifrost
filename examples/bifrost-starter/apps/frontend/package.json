{
  "name": "@bifrost-starter/frontend",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "./gen_env.sh local && next dev -p 3000",
    "lint": "eslint --cache --cache-location .next/cache/eslint/",
    "lint:fix": "pnpm lint --fix",
    "lint:style": "stylelint './**/*.module.css'",
    "lint:style:fix": "stylelint --fix './**/*.module.css'",
    "postinstall": "test -f .env.local || (test -f .env.sample && cp .env.sample .env.local || echo -e \"\\e[33mNo env file initialized\\e[0m\")",
    "start": "./gen_env.sh production && next start",
    "test": "jest",
    "test:circular": "pnpm depcruise --validate .dependency-cruiser.js pages --ignore-known",
    "test:coverage": "pnpm test -- --coverage",
    "test:type": "pnpm build"
  },
  "dependencies": {
    "@bifrost-starter/interfaces": "workspace:*",
    "axios": "0.27.2",
    "axios-auth-refresh": "3.3.3",
    "jwt-decode": "3.1.2",
    "next": "12.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.35.0",
    "react-intl": "6.1.1",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@axe-core/react": "4.4.4",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.0.3",
    "@types/jest-axe": "3.5.4",
    "@types/node": "17.0.25",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "dependency-cruiser": "11.15.0",
    "dependency-cruiser-config-custom": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "jest": "29.0.3",
    "jest-axe": "6.0.0",
    "jest-environment-jsdom": "29.0.3",
    "prettier": "2.7.1",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "28.0.0",
    "stylelint-declaration-strict-value": "1.9.0",
    "ts-node": "10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": "^18.12.1"
  }
}
