{
  "name": "@bifrost-starter/backend",
  "description": "This a project doing cool stuff",
  "private": true,
  "scripts": {
    "build": "nest build -p tsconfig.build.json",
    "build:entities": "nest build -p tsconfig.typeorm.json",
    "dev": ". ./.env.rc && docker-compose up -d && nest start -p ./tsconfig.json src/main.ts --watch --preserveWatchOutput",
    "explore": "pnpm start --entryFile repl",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --cache './**/*.{js,ts,tsx}' --max-warnings 0",
    "lint:fix": "pnpm lint --fix",
    "migration:generate": ". ./.env.rc && typeorm --outputJs -d datasource.js migration:generate",
    "migration:revert": ". ./.env.rc && typeorm -d datasource.js migration:revert",
    "migration:run": ". ./.env.rc && typeorm -d datasource.js migration:run",
    "migration:run:prod": "typeorm -d datasource.js migration:run",
    "postinstall": "test -f .env.rc || cp .env.example.rc .env.rc",
    "premigration:generate": "pnpm build:entities",
    "reset": "rm -rf node_modules && pnpm install",
    "schema:drop": ". ./.env.rc && typeorm -d datasource.js schema:drop",
    "start:debug": "pnpm start --debug",
    "start:prod": "node dist/main.js",
    "test": ". ./.env.test.rc && jest * --maxWorkers=2",
    "test:circular": "depcruise --validate .dependency-cruiser.js src",
    "test:coverage": "docker-compose up -d db-test && pnpm test -- --coverage",
    "test:debug": "NODE_OPTIONS='--inspect-brk' pnpm test",
    "test:schema:drop": ". ./.env.test.rc && TYPEORM_ENTITIES='' typeorm -d datasource.js schema:drop",
    "test:type": "pnpm build",
    "test:watch": "pnpm test --watch"
  },
  "dependencies": {
    "@bifrost-starter/interfaces": "workspace:*",
    "@nestjs/common": "9.0.11",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.11",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mapped-types": "1.1.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.11",
    "@nestjs/swagger": "6.1.2",
    "@nestjs/typeorm": "9.0.1",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cli-color": "2.0.3",
    "cookie-parser": "1.4.6",
    "express": "4.18.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.6",
    "swagger-ui-express": "4.5.0",
    "typeorm": "0.3.9",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.3",
    "@nestjs/testing": "9.0.11",
    "@types/bcrypt": "5.0.0",
    "@types/cli-color": "2.0.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.14",
    "@types/faker": "5.5.9",
    "@types/jest": "29.0.3",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.14.185",
    "@types/node": "16.11.59",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@types/validator": "13.7.6",
    "dependency-cruiser": "11.15.0",
    "dependency-cruiser-config-custom": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "faker": "5.5.3",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "tsconfig": "workspace:*",
    "typescript": "4.7.4",
    "webpack": "5.74.0"
  },
  "engines": {
    "node": "^18.12.1"
  }
}
