{
  "name": "@bifrost-starter/backend",
  "description": "This a project doing cool stuff",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm gen:metadata && nest build -p tsconfig.build.json",
    "build:entities": "nest build -p tsconfig.entities.json",
    "dev": ". ./.env.rc && docker-compose up -d && pnpm gen:metadata && nest start -p ./tsconfig.json src/main.ts --watch --preserveWatchOutput",
    "gen:metadata": "ts-node src/generate-metadata.ts",
    "explore": "pnpm start --entryFile repl",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --cache './**/*.{js,ts,tsx}' --max-warnings 0",
    "lint:fix": "pnpm lint --fix",
    "migration:generate": ". ./.env.rc && typeorm --outputJs -d datasource.js migration:generate",
    "migration:revert": ". ./.env.rc && typeorm -d datasource.js migration:revert",
    "migration:run": ". ./.env.rc && typeorm -d datasource.js migration:run",
    "migration:run:prod": "typeorm -d datasource.js migration:run",
    "postinstall": "test -f .env.rc || (test -f .env.example.rc && cp .env.example.rc .env.rc || echo -e \"\\e[33mNo env file initialized\\e[0m\")",
    "premigration:generate": "pnpm build:entities",
    "reset": "rm -rf node_modules && pnpm install",
    "schema:drop": ". ./.env.rc && typeorm -d datasource.js schema:drop",
    "start:debug": "pnpm start --debug",
    "start:prod": "node dist/main.js",
    "test": ". ./.env.test.rc && jest * --maxWorkers=2",
    "test:circular": "depcruise --validate .dependency-cruiser.js src",
    "test:coverage": "docker-compose up -d db-test && pnpm test -- --coverage",
    "test:debug": "NODE_OPTIONS='--inspect-brk' pnpm test",
    "test:schema:drop": ". ./.env.test.rc && TYPEORM_ENTITIES='' typeorm -d datasource.js schema:drop",
    "test:type": "pnpm build",
    "test:watch": "pnpm test --watch"
  },
  "dependencies": {
    "@adminjs/express": "5.0.1",
    "@adminjs/nestjs": "5.1.0",
    "@adminjs/typeorm": "4.0.0",
    "@bifrost-starter/interfaces": "workspace:*",
    "@nestjs/common": "10.2.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.10",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "2.0.4",
    "@nestjs/passport": "10.0.2",
    "@nestjs/platform-express": "10.2.10",
    "@nestjs/swagger": "7.1.17",
    "@nestjs/typeorm": "10.0.1",
    "adminjs": "6.7.5",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cli-color": "2.0.3",
    "connect-typeorm": "2.0.0",
    "cookie-parser": "1.4.6",
    "express": "4.18.2",
    "express-formidable": "1.2.0",
    "express-session": "1.17.3",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "pg": "8.11.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "swagger-ui-express": "4.6.3",
    "typeorm": "0.3.9",
    "winston": "3.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/testing": "10.2.10",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.99",
    "@types/bcrypt": "5.0.0",
    "@types/cli-color": "2.0.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/faker": "5.5.9",
    "@types/jest": "29.5.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/lodash": "4.14.195",
    "@types/node": "20.10.5",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@types/validator": "13.7.17",
    "dependency-cruiser": "15.5.0",
    "dependency-cruiser-config-custom": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "faker": "5.5.3",
    "jest": "29.5.0",
    "prettier": "3.1.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.2",
    "tsconfig": "workspace:*",
    "typescript": "5.3.3",
    "webpack": "5.88.1"
  },
  "engines": {
    "node": "^20.10.0"
  }
}
